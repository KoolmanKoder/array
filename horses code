import java.util.ArrayList;

public class Main { 
  public static void main(String[] args) { 
    ArrayList<String> horses = new ArrayList<String>();
    horses.add("Volvo who is 3");
    horses.add("BMW who is 20");
    horses.add("Ford who is 1 1/2");
    System.out.println("My horses are"+horses);
  } 
}

